CC = c++
CFLAGS = -Wall -Wextra -Werror -std=c++98
TARGET = replace
OBJS = main.cpp 

all: $(TARGET)

$(TARGET): $(OBJS)
	$(CC) $(CFLAGS) -o $@ $^

clean:
	rm -f $(OBJS)

fclean: clean
	rm -f $(TARGET)

re: fclean all

.PHONY: all clean fclean re
